@{ ViewBag.Title = "Oops! Error Encountered"; }


<section id="inner-headline">
	<div class="container">
		<div class="row">
			<div class="col-lg-12">
				<ul class="breadcrumb">
					<li><a href="~/Home/Index"><i class="fa fa-home"></i></a><i class="icon-angle-right"></i></li>
					<li class="active">@Response.Status</li>
				</ul>
			</div>
		</div>
	</div>
</section>

<section id="content">
	<div class="container">
		<div class="row demobtn">
			<div class="col-lg-12">
				<div class="row">
					<div class="col-lg-12">
                        <div class="alert alert-danger">
                            <strong>HTTP @Response.StatusCode</strong> - @Response.StatusDescription
						</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>